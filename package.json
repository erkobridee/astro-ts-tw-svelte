{
  "name": "astro-ts-tw-svelte",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "unset:git-hooks-path": "git config --unset core.hooksPath",
    "simple-git-hooks": "npx simple-git-hooks",
    "postinstall": "is-ci || npm run simple-git-hooks",
    "prettier": "prettier --write  \"**/*.{js,jsx,ts,tsx,md,mdx,astro,svelte}\"",
    "eslint": "eslint --fix \"src/**/*.{js,ts,jsx,tsx,astro,svelte}\"",
    "lint": "run-s prettier eslint",
    "dev": "astro dev --host",
    "start": "astro dev --host",
    "build": "astro check && astro build",
    "preview": "astro preview --host",
    "sync": "astro sync",
    "astro": "astro",
    "gh-pages-build": "cross-env GITHUB_PAGES=true npm run build",
    "gh-pages-preview": "cross-env GITHUB_PAGES=true GITHUB_PAGES_PREVIEW=true npm run preview"
  },
  "dependencies": {
    "astro": "5.12.8",
    "canvas-confetti": "1.9.3",
    "dayjs": "1.11.13",
    "echarts": "6.0.0",
    "svelte": "4.2.19"
  },
  "devDependencies": {
    "@astrojs/check": "0.9.4",
    "@astrojs/mdx": "4.3.3",
    "@astrojs/svelte": "5.7.3",
    "@astrojs/ts-plugin": "1.10.4",
    "@eslint/compat": "1.3.1",
    "@tailwindcss/typography": "0.5.16",
    "@tailwindcss/vite": "4.1.11",
    "@types/canvas-confetti": "1.9.0",
    "@typescript-eslint/eslint-plugin": "8.38.0",
    "@typescript-eslint/parser": "8.38.0",
    "autoprefixer": "10.4.21",
    "clsx": "2.1.1",
    "cross-env": "10.0.0",
    "eslint": "9.32.0",
    "eslint-plugin-astro": "1.3.1",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-svelte": "3.11.0",
    "globals": "16.3.0",
    "is-ci": "4.1.0",
    "lint-staged": "16.1.4",
    "npm-run-all2": "8.0.4",
    "postcss": "8.5.6",
    "prettier": "3.6.2",
    "prettier-plugin-astro": "0.14.1",
    "prettier-plugin-svelte": "3.4.0",
    "prettier-plugin-tailwindcss": "0.6.14",
    "simple-git-hooks": "2.13.1",
    "tailwind-merge": "3.3.1",
    "tailwindcss": "4.1.11",
    "typescript": "5.9.2"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx,md,mdx,astro,svelte}": "prettier --write",
    "src/**/*.{js,ts,jsx,tsx,astro,svelte}": "eslint --fix"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "github_pages": "https://erkobridee.github.io"
}