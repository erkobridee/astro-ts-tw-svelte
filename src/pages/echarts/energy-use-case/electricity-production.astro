---
import Layout from '~/layouts/ApacheEChartsLayout.astro';

import Example from '~/components/apache-echarts/energy-use-case/ElectricityProduction.svelte';

import {
  type TimeSerie,
  generateElectricityProductionDayData
} from '~/utils/timeseries';

const timeseries: TimeSerie[] =
  generateElectricityProductionDayData().minutes.plain;
---

<Layout title="Energy Use Case - Electricity Production">
  <Example {timeseries} client:load />
</Layout>
